{
  "client/scenes/!(index).js": {
    "filenameRule": {
      "endsWith": "Scene.js"
    }
  },
  "client/ui/admin/**/*.jsx": {
    "filenameRule": {
      "startsWith": "Admin"
    }
  },
  "client/ui/bounds/admin/**/*.jsx": {
    "filenameRule": {
      "startsWith": "Admin"
    }
  },
  "client/ui/bounds/sign/*.jsx": {
    "filenameRule": {
      "startsWith": "Sign"
    }
  },
  "client/ui/views/**/!(index).jsx": {
    "filenameRule": {
      "endsWith": "View.jsx"
    }
  },
  "conf/Urls.js": {
    "moduleRule": {
      "type": "object",
      "valuePattern": "/^//",
      "valueUnique": true
    }
  },
  "server/**/*.js": {
    "contentRule": {
      "endsWithNewLine": true
    }
  },
  "server/controllers/!(index).js": {
    "filenameRule": {
      "endsWith": "Ctrl.js"
    },
    "moduleRule": {
      "type": "function"
    }
  },
  "server/db/hooks/!(index).js": {
    "filenameRule": {
      "endsWith": "Hook.js"
    },
    "moduleRule": {
      "type": "function"
    }
  },
  "server/db/resources/!(index).js": {
    "filenameRule": {
      "endsWith": "Resource.js"
    }
  },
  "server/db/seeds/**/!(index|_*).js": {
    "filenameRule": {
      "endsWith": ".seed.js"
    }
  }
}