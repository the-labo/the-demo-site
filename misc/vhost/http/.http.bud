'use strict'

const {APP_DOMAIN: domain, APP_PORT: port} = require('@self/Local')

module.exports = [
  domain,
  ['www', domain].join('.')
].map((domain) => ({
  force: true,
  mode: '444',
  path: `${domain}.http`,
  data: {
    domain,
    port,
    root: __dirname
  }
}))

if (!module.parent) {
  require('coz').render(__filename)
}